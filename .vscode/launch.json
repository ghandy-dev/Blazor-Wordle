{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch and Debug Standalone Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "cwd": "${workspaceFolder}/Wordle.Blazor",
            "url": "http://localhost:5146",
        },
        {
            "name": "Attach and Debug",
            "type": "blazorwasm",
            "request": "attach",
            "url": "http://localhost:5146"
        }
    ]
}